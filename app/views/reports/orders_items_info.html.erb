<table>
  <tr>
    <th>Item Name</th>
    <th>Purchase Part ID</th>
    <th>Color</th>
    <th>Quantity</th>
    <th>Width</th>
    <th>Height</th>
    <th>Depth</th> 
  </tr>
  
  <%#= raise @report_data.inspect %>
  <% @report_data.each_pair do |key, job| %>
    <tr>
      <th>
        <%=job[:name]%>
      </th>
    </tr>
    <% job[:items].each do |job_item| %>
    <tr>
      <td><%= job_item[:item_name] %></td>
      <td><%= job_item[:purchase_part_id] %></td>
      <td><%= job_item[:color] %></td>
      <td><%= job_item[:quantity] %></td>
      <td><%= job_item[:width] %></td>
      <td><%= job_item[:height] %></td>
      <td><%= job_item[:depth] %></td>
    </tr>
    <% end %>
  <% 
  end
  %>
  
</table>