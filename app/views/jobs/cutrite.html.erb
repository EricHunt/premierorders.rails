<p id="notice"><%= notice %></p>

<table class="cutrite_csv_header">
  <tr>
    <% @job.cutrite_job_header.each do |label| -%><th><%= label -%></th><% end %>
  </tr>
  <tr>
    <% @job.cutrite_job_data.each do |value| -%><td><%= value -%></td><% end %>
  </tr>
</table>
<table class="cutrite_csv_items">
  <tr>
    <% @job.cutrite_items_header.each do |label| -%><th><%= label -%></th><% end %>
  </tr>
  <% @job.cutrite_items_data.each do |row| -%>
    <tr>
      <% row.each do |col| -%><td><%= col -%></td><% end %>
    </tr>
  <% end %>
</table>

<%= link_to 'Edit', edit_job_path(@job) %> |
<%= link_to 'Download', download_job_path(@job) %> |
<%= link_to 'Back', jobs_path %>
